ControlMaster auto
ControlPath /tmp/%r@%h:%p
ControlPersist yes

Host *
    SendEnv LANG LC_*
    ServerAliveInterval 60

Host *+*
    ProxyCommand ssh $(echo %h | sed 's/+[^+]*$//;s/\([^+%%]*\)%%\([^+]*\)$/\2 -l \1/;s/:/ -p /') nc -w120 $(echo %h | sed 's/^.*+//;/:/!s/$/ %p/;s/:/ /')

Host *.predicative.com 66.209.184.* 10.5.4.* 33.33.33.42 33.33.33.50 ec2-*
    ForwardAgent yes
    UserKnownHostsFile=~/.ssh/known_hosts_cert

Host mgmt.predicative.com 10.5.4.167
    SendEnv AWS_*

Host 192.168.12.* 10.7.4.* 10.6.3.*
    ForwardAgent yes
    UserKnownHostsFile=~/.ssh/known_hosts_mine
